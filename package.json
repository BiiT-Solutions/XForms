{
  "name": "xforms",
  "version": "1.0.230",
  "scripts": {
    "ng": "ng",
    "start": "npm run prepare:translations && ng serve",
    "build": "npm run prepare:translations && ng build --output-hashing=all --configuration docker --base-href /xforms",
    "watch": "npm run prepare:translations && ng build --watch --configuration development",
    "test": "ng test",
    "prepare:docs": "cpx ./README.md ./dist/xforms && echo \"{\\\"name\\\": \\\"`node -e \"console.log(require('./package.json').name);\"\n`\\\", \\\"version\\\": \\\"`node -e \"console.log(require('./package.json').version);\"\n`\\\"}\" > ./dist/xforms/package.json",
    "prepare:publication": "npm run prepare:translations && npm run build && npm run prepare:docs",
    "clean:translations": "del-cli --force ./src/assets/i18n/biit-ui/ && del-cli --force ./src/assets/i18n/xforms/",
    "prepare:translations": "npm run clean:translations && cpx \"./node_modules/biit-ui/assets/i18n/**/*.json\" \"./src/assets/i18n/biit-ui/\" && cpx \"./node_modules/x-forms-lib/assets/i18n/**/*.json\" \"./src/assets/i18n/xforms/\"",
    "publish": "npm run prepare:publication && npm publish ./dist/xforms"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "v15-lts",
    "@angular/common": "v15-lts",
    "@angular/compiler": "v15-lts",
    "@angular/core": "v15-lts",
    "@angular/forms": "v15-lts",
    "@angular/platform-browser": "v15-lts",
    "@angular/platform-browser-dynamic": "v15-lts",
    "@angular/router": "v15-lts",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0",
    "biit-ui": "latest",
    "x-forms-lib": "latest",
    "kafka-event-structure-lib": "latest",
    "user-manager-structure-lib": "latest",
    "fast-copy": "3.0.2"
  },
  "devDependencies": {
    "del-cli": "^5.1.0",
    "cpx": "^1.5.0",
    "@angular-devkit/build-angular": "v15-lts",
    "@angular/cli": "v15-lts",
    "@angular/compiler-cli": "v15-lts",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.9.4"
  }
}
